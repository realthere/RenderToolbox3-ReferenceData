<?xml version="1.0" encoding="utf-8"?><!-- basic renderer properties that Collada doesn't know about -->
<pbrt_xml>
   <Material id="CubeMaterial-material" type="plastic">
      <parameter name="Kd" type="spectrum">/home2/brainard/toolboxes/RenderToolbox3/RenderData/Macbeth-ColorChecker/mccBabel-4.spd</parameter>
      <parameter name="Ks" type="spectrum">300:0.75 800:0.75</parameter>
      <parameter name="roughness" type="float">0.003</parameter>
   </Material>
   <Material id="SphereMaterial-material" type="anisoward">
      <parameter name="Kd" type="texture">earthTexture</parameter>
      <parameter name="Ks" type="spectrum">300:0 800:0</parameter>
      <parameter name="alphaU" type="float">0.1</parameter>
      <parameter name="alphaV" type="float">0.1</parameter>
   </Material>
   <Texture id="earthTexture" type="imagemap">
      <parameter name="filename" type="string">/home2/brainard/toolboxes/RenderToolbox3/ExampleScenes/CubanSphere/earthbump1k-stretch-rgb.exr</parameter>
      <parameter name="wrap" type="string">repeat</parameter>
      <parameter name="gamma" type="float">1</parameter>
      <parameter name="trilinear" type="bool">false</parameter>
      <parameter name="maxanisotropy" type="float">20</parameter>
      <parameter name="udelta" type="float">0.0</parameter>
      <parameter name="vdelta" type="float">0.0</parameter>
      <parameter name="uscale" type="float">1.0</parameter>
      <parameter name="vscale" type="float">1.0</parameter>
      <parameter name="dataType" type="string">spectrum</parameter>
   </Texture>
   <merge id="Sun2-light" type="distant">
      <parameter name="L" type="spectrum">400:1 700:10</parameter>
   </merge>
   <merge id="Sun1-light" type="distant">
      <parameter name="L" type="spectrum">400:10 700:1</parameter>
   </merge>
   <merge id="RearLamp-light" type="point">
      <parameter name="I" type="spectrum">/home2/brainard/toolboxes/RenderToolbox3/RenderData/D65.spd</parameter>
   </merge>
   <merge id="TopLamp-light" type="point">
      <parameter name="I" type="spectrum">/home2/brainard/toolboxes/RenderToolbox3/RenderData/D65.spd</parameter>
   </merge>

    
   <SurfaceIntegrator id="integrator" type="directlighting"/>

    
   <Sampler id="sampler" type="lowdiscrepancy">
        
      <parameter name="pixelsamples" type="integer">8</parameter>
    
   </Sampler>
    
    
   <PixelFilter id="filter" type="gaussian">
        
      <parameter name="alpha" type="float">2</parameter>
        
      <parameter name="xwidth" type="float">2</parameter>
        
      <parameter name="ywidth" type="float">2</parameter>
    
   </PixelFilter>
    

</pbrt_xml>